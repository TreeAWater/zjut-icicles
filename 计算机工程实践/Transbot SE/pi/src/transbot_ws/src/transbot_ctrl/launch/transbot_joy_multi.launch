<launch>
    <arg name="namespace" default="robot1"/>
    <param name="use_sim_time" value="false"/>
    <group ns="$(arg namespace)">
        <node name="joy_node" pkg="joy" type="joy_node" output="screen" respawn="false">
                    <remap from="/joy" to="joy"/>
         </node>   
        <node name="transbot_joy_node" pkg="transbot_ctrl" type="transbot_joy.py" output="screen" respawn="false">
        <remap from="/joy" to="joy"/>
        <remap from="/cmd_vel" to="cmd_vel"/>
        <remap from="/TargetAngle" to="TargetAngle"/>
        <!--remap from="/PWMServo" to="PWMServo"/-->
        </node>
    </group>
</launch>
